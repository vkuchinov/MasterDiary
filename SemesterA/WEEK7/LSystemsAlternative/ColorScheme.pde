//based on Adobe Kuler swatches
class ColorScheme{
  
    color[] colors = {#EB5937, #1C1919, #403D3C, #456F74, #D3CBBD };
    
    color background, strokeA, strokeB, strokeC;
    
    ColorScheme(){
     
          strokeA = colors[4];
          strokeB = colors[3];
          strokeC = colors[0];
          //highlightedStroke = colors[0];
          background = colors[1];
     
    } 
  
}
