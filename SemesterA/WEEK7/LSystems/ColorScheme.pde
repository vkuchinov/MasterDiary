//based on Adobe Kuler swatches
class ColorScheme{
  
    color[] colors = {#EB5937, #1C1919, #403D3C, #456F74, #D3CBBD };
    
    color background, stroke, highlightedStroke;
    
    ColorScheme(){
     
          stroke = colors[4];
          highlightedStroke = colors[0];
          background = colors[1];
     
    } 
  
}
